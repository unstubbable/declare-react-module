{
  "name": "declare-react-module",
  "version": "0.0.0-development",
  "description": "Create TypeScript declaration files for React modules written in ES6.",
  "main": "dist/index.js",
  "bin": "cli.js",
  "files": [
    "cli.js",
    "dist",
    "typings"
  ],
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "tsc --project .",
    "watch": "yarn run build -- --watch",
    "format": "tsfmt --replace",
    "verify": "tsfmt --verify && tslint --config tslint.json --project tsconfig.json --type-check --formatters-dir node_modules/custom-tslint-formatters/formatters --format grouped \"{src,typings}/**/*.{ts,tsx}\"",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KingHenne/declare-react-module.git"
  },
  "keywords": [
    "typescript",
    "react",
    "declaration",
    "definition",
    "dts",
    "react2dts",
    "es6"
  ],
  "author": "Hendrik Liebau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KingHenne/declare-react-module/issues"
  },
  "homepage": "https://github.com/KingHenne/declare-react-module#readme",
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "codecov": "^2.1.0",
    "commitizen": "^2.9.6",
    "custom-tslint-formatters": "^2.1.0",
    "cz-customizable": "^5.0.0",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.2",
    "ts-config": "^5.0.0",
    "ts-jest": "^19.0.10",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "typescript-formatter": "^5.1.3"
  },
  "dependencies": {
    "@types/acorn": "^4.0.2",
    "@types/babel-core": "^6.7.14",
    "@types/fs-promise": "^1.0.2",
    "@types/minimatch": "^2.0.29",
    "@types/node": "^7.0.12",
    "@types/resolve": "^0.0.4",
    "fs-promise": "^2.0.2",
    "jest": "next",
    "minimatch": "^3.0.3",
    "react-to-typescript-definitions": "^0.22.0",
    "rollup": "^0.41.6",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "tslib": "^1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "lint-staged": {
    "{src,typings}/**/*.{ts,tsx}": [
      "tsfmt --replace",
      "tslint --config tslint.json --project tsconfig.json --type-check",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testRegex": "/__tests__/.*\\.test\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  }
}
